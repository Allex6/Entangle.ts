{
  "name": "entangle.ts",
  "version": "0.1.0",
  "description": "A declarative, event-driven framework for orchestrating business logic in TypeScript & Node.js applications.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.{ts,json}\"",
    "prepare": "husky",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "keywords": [
    "framework",
    "orchestration",
    "event-driven",
    "decoupling",
    "typescript",
    "node",
    "ioc",
    "di"
  ],
  "author": "Alex Souza <alex.webdevelloper@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/allex6/entangle.ts.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@faker-js/faker": "^9.9.0",
    "@types/node": "^22.15.30",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "@vitest/coverage-v8": "^3.2.4",
    "commitlint-config-gitmoji": "^2.3.1",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.36.0",
    "vitest": "^3.2.4"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ]
}
